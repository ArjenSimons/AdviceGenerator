<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/homeFragment2">

    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.example.random_advice_generator.view.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment2_to_generateFragment2"
            app:destination="@id/generateFragment2" />
    </fragment>
    <fragment
        android:id="@+id/generateFragment2"
        android:name="com.example.random_advice_generator.view.GenerateFragment"
        android:label="Generate advice"
        tools:layout="@layout/fragment_generate" >
        <action
            android:id="@+id/action_generateFragment2_to_homeFragment2"
            app:destination="@id/homeFragment2" />
    </fragment>
</navigation>